<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.5.2"/>
    <httpRuntime targetFramework="4.5.2"/>
  </system.web>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="ndfdXMLBinding">
          <security mode="Transport" />
        </binding>
        <binding name="ndfdXMLBinding1" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="https://graphical.weather.gov:443/xml/SOAP_server/ndfdXMLserver.php"
        binding="basicHttpBinding" bindingConfiguration="ndfdXMLBinding"
        contract="USAWeather.ndfdXMLPortType" name="ndfdXMLPort" />
    </client>
  </system.serviceModel>
</configuration>